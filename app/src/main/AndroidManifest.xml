<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ml_gram.project_coordination_final">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_smartphone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_smartphone_round"
        android:supportsRtl="true"
        android:requestLegacyExternalStorage="true"
        android:theme="@style/Theme.Project_coordination_2">
        <activity android:name="com.ml_gram.project_coordination_final.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ml_gram.project_coordination_final.GraphActivity" android:label="Graph analisys"></activity>
        <activity android:name="com.ml_gram.project_coordination_final.OpencvActivity" android:label="OpenCV Test"></activity>
    </application>

</manifest>